# Database Configuration
DATABASE_URL=postgres://username:password@localhost:5432/ridewire

# JWT Secret (use a strong random string)
JWT_SECRET=your_jwt_secret_key_here_change_in_production

# Server Port
PORT=3000

# Node Environment
NODE_ENV=development

# React App API URL (leave empty for same-origin, or set for different backend)
REACT_APP_API_URL=

# Webpack Dev Server Configuration
DEV_SERVER_PORT=3001
API_PROXY_URL=http://localhost:3000

# Supabase Configuration (if using Supabase)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here

# ============================================
# AI PROVIDER API KEYS
# ============================================

# === OPEN-SOURCE LLM PROVIDERS (NEW - Cost Savings: $690/month) ===

# Cerebras API Key (FREE - Unlimited, Fastest Inference)
# Get at: https://cloud.cerebras.ai
CEREBRAS_API_KEY=csk-your-cerebras-api-key-here

# Groq API Key (FREE - 14,400 requests/day)
# Get at: https://console.groq.com
GROQ_API_KEY=gsk_your-groq-api-key-here

# Together.ai API Key ($25 FREE credit, then ~$0.0012/query)
# Get at: https://api.together.xyz
TOGETHER_API_KEY=your-together-api-key-here

# === LEGACY/TIEBREAKER PROVIDERS ===

# OpenAI API Key (Used as tiebreaker only with open-source orchestrator)
OPENAI_API_KEY=sk-your-api-key-here

# Anthropic API Key (Legacy - for old orchestrator)
ANTHROPIC_API_KEY=sk-ant-your-api-key-here

# Google API Key (Legacy - for old orchestrator)
GOOGLE_API_KEY=your-google-api-key-here

# === AI ORCHESTRATOR SELECTION ===
# Set to "true" to use open-source LLMs (Cerebras, Groq, Together.ai)
# Set to "false" to use legacy proprietary APIs (OpenAI, Anthropic, Google)
USE_OPEN_SOURCE_LLMS=true

# Stripe Configuration (for payments)
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key

# Gumroad Configuration (for marketplace)
GUMROAD_ACCESS_TOKEN=your-gumroad-access-token
GUMROAD_AUTO_PUBLISH=true

# E-Commerce Automation Settings
AUTO_LIST_DIAGRAMS=true
PLATFORM_FEE_PERCENTAGE=0.30

# Admin API Key (for admin endpoints)
ADMIN_API_KEY=your-secure-admin-key-change-in-production
# COCO AI Content Generation
ELEVENLABS_API_KEY=your-elevenlabs-api-key-here
DID_API_KEY=your-did-api-key-here

# YouTube Integration
YOUTUBE_CLIENT_ID=your-youtube-client-id
YOUTUBE_CLIENT_SECRET=your-youtube-client-secret
YOUTUBE_CHANNEL_ID=your-youtube-channel-id

# Gumroad Integration
GUMROAD_API_KEY=your-gumroad-api-key

# Stripe Payment Processing
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key

# Email Configuration
SMTP_HOST=smtp.your-provider.com
SMTP_PORT=587
SMTP_USER=your-email@domain.com
SMTP_PASSWORD=your-smtp-password

# Automation Settings
AUTOMATION_ACTIVE=false
COCO_ENABLED=false
GUMROAD_SYNC_ENABLED=false
